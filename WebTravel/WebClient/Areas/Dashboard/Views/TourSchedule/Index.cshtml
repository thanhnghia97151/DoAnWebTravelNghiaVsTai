@model List<TourSchedule>
    <link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />
<a asp-action="Create" class="btn btn-success">Create</a>
<table class="table table-bordered">
    <thead class="table-dark"> 
        <tr>

            <th>Content</th>
            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
       {
           <tr>

               <td>@Html.Raw(System.Web.HttpUtility.HtmlDecode(item.Content))</td>
               <td>
                    <a asp-action="Edit" asp-route-id="@item.TourScheduleId"><i class="fa fa-edit"></i></a>
                </td>
                <td>
                    <a href="/dashboard/news/confirmdelete/@item.TourScheduleId"><i class="fa fa-trash"></i></a>
                </td>
           </tr>
       }
    </tbody>
</table>
<script src="~/js/jquery.dataTables.min.js"></script>
<script>
    $('.table').DataTable();
</script>