@model List<About>
<a asp-action="Create" class="btn btn-success">Create</a>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Meta Title</th>
            <th>Content</th>
            <th>Image</th>
            <th>Detail</th>
            <th>Note</th>
            <th>Created Date</th>
            <th>Created by</th>
            <th>Modified Date</th>
            <th>Modified By</th>
            <th> Policy Id</th>
            <th>Status </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.MetaTitle</td>
                <td>@item.Content</td>
                <td>@item.Image</td>
                <td>@item.Detail</td>
                <td>@item.Note</td>
                <td>@item.CreatedDate</td>
                <td>@item.CreatedBy</td>
                <td>@item.ModifiedDate</td>
                <td>@item.ModifiedBy</td>
                <td>@item.PolicyId</td>
                <td>@item.Status</td>
                <td>
                      <a asp-action="Edit" asp-route-id="@item.Id"><i class="fa fa-edit"></i></a>
                   <a href="/dashboard/about/confirmdelete/@item.Id"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>