@model List<NewsCategory>
<a class="btn btn-success" asp-action="Create" >Create</a>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Seo Title</th>
            <th>Meta Title</th>
            <th>ParentId</th>
            <th>Created Date</th>
            <th>Created By</th>
            <th>Modified Date</th>
            <th>Modified By</th>
            <th>Status</th>
            <th>Show On Home</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.NewsCategoryId</td>
                <td>@item.Name</td>
                <td>@item.SeoTitle</td>
                <td>@item.MetaTitle</td>
                <td>@item.ParentId</td>
                <td>@item.CreatedDate</td>
                <td>@item.CreatedBy</td>
                <td>@item.ModifiedDate</td>
                <td>@item.ModifiedBy</td>
                <td>@item.Status</td>
                <td>@item.ShowOnHome</td>
            </tr>
        }
    </tbody>
</table>