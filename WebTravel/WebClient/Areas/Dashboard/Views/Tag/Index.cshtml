@model List<Tag>
    <link href="~/css/jquery.dataTables.min.css" rel="stylesheet" />

<a class="btn btn-success" asp-action="Create">Create</a>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>

            <th>Name</th>
            <th>Edit</th>
            <th>Delete</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>

                <td>@item.Name</td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.TagId"><i class="fa fa-edit"></i></a>
                </td>
                <td>
                    <a href="/dashboard/news/confirmdelete/@item.TagId"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
<script src="~/js/jquery.dataTables.min.js"></script>
<script>
    $('.table').DataTable();
</script>