@model List<Member>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>MemberID</th>
            <th>UserName</th>
            <th>Roles</th>
            <th></th>
        </tr>
        
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.MemberID</td>
                <td>@item.UserName</td>
                <td>
                    <a href="/dashboard/member/role/@item.MemberID">Roles</a>
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.MemberID"><i class="fa fa-edit"></i></a>
                    <a href="/dashboard/member/confirmdelete/@item.MemberID"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>