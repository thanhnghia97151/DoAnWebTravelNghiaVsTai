        }

        if (feDichVu == 0) {
            alert('Vui lòng chọn dịch vụ');
        }
        else if (feDestination == 0) {
            alert('Vui lòng chọn điểm đến');
        }
        else {
            window.open('http://fe.travel.com.vn/tim-tour/0/' + feTourType + '/0/' + feDestination + '/' + feDichVu + '/' + date + '/ket-qua');
        }
    }

